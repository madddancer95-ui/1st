{
  "version": 2,
  "name": "delhitattooshop-live", 
  "alias": ["delhitattooshop.com", "www.delhitattooshop.com"],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://www.delhitattooshop.com/:path*",
      "statusCode": 301,
      "has": [
        {
          "type": "host",
          "value": "delhitattooshop.com"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "VERCEL_FORCE_REBUILD": "99",
      "BUILD_TIMESTAMP": "2025-09-08-04-30-FINAL-FORCE",
      "DOMAIN_TRANSFER": "LIVE_NOW", 
      "CACHE_BUST": "NUCLEAR_REBUILD",
      "NODE_ENV": "production"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "repo": "madddancer95-ui/1st",
    "autoAlias": true,
    "autoJobCancelation": true
  },
  "cleanUrls": true,
  "trailingSlash": false
}