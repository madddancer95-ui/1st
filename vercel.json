{
  "version": 2,
  "name": "delhitattooshop-live", 
  "alias": ["delhitattooshop.com", "www.delhitattooshop.com"],
  "build": {
    "env": {
      "VERCEL_FORCE_REBUILD": "4",
      "BUILD_TIMESTAMP": "2025-09-08-00-50",
      "DOMAIN_TRANSFER": "LIVE_NOW", 
      "CACHE_BUST": "DOMAIN_SWITCH"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "repo": "madddancer95-ui/1st",
    "autoAlias": true,
    "autoJobCancelation": true
  },
  "cleanUrls": true,
  "trailingSlash": false
}