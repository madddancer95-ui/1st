{
  "build": {
    "env": {
      "VERCEL_FORCE_REBUILD": "3",
      "BUILD_TIMESTAMP": "2025-09-07-23-55",
      "FORCE_DEPLOY": "GITHUB_DIRECT_CONNECTION",
      "CACHE_BUST": "REBUILD_NOW"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "autoAlias": true,
    "autoJobCancelation": true
  },
  "cleanUrls": true,
  "trailingSlash": false
}